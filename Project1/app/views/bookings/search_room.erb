<h1>Search Room</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">

      <%= form_for @rooms, url: search_room_path do |f| %>

          <%= f.label :size %>
          <%= f.select :size, [nil, 4, 6, 12] %>

          <%= f.label :building %>
          <%= f.select :building, [nil, "James B. Hunt", "D. H. Hill"] %>

          <%= f.label :room_id %>
          <%= f.text_field :room_id %>

          <%= f.submit "Search", class: "btn btn-primary" %>

  <% end %>
  </div>
</div>

